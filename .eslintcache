r = $this.innerHTML;\n            letter = letter.trim();\n            var delay = 70;\n            var delayArray = new Array;\n            var randLetter = new Array;\n            for (let j = 0; j < letter.length; j++) {\n                while (1) {\n                    var random = getRandomInt(0, (letter.length - 1));\n                    if (delayArray.indexOf(random) == -1)\n                        break;\n                }\n                delayArray[j] = random;\n            }\n            for (let l = 0; l < delayArray.length; l++) {\n                var str = '';\n                var index = delayArray[l];\n                if (letter[index] != ' ') {\n                    str = '<span style=\"animation-delay:' + delay + 'ms; -moz-animation-delay:' + delay + 'ms; -webkit-animation-delay:' + delay + 'ms; \">' + letter[index] + '</span>';\n                    randLetter[index] = str;\n                } else\n                    randLetter[index] = letter[index];\n                delay += 80;\n            }\n            randLetter = randLetter.join(\"\");\n            $this.innerHTML = randLetter;\n        }\n    }\n    \n    function getRandomInt(min, max) {\n        return Math.floor(Math.random() * (max - min + 1)) + min;\n    }\n    return (\n        <div className={classes.root}>\n            <Container maxWidth=\"md\">\n                    <section \n                        className={classes.logo}\n                    >\n                        <Logo title=\"nicole remy\"/>\n                    </section>\n                    <section className=\"welcome-container\">\n                        <h1 className=\"cssanimation sequence leRotateSkateInLeft\">welcome</h1>\n                    </section>\n                    \n                    <section className={classes.titles}>\n                        <h1>full stack developer  · collaborator  · innovator  · learner</h1>\n                    </section>\n                    <section \n                        